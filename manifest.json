{
    "manifest_version": 2,
    "name": "LeetCode Reminder",
    "version": "1.0",
    "description": "A way to re-visit concepts",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "permissions": [
        "history"
    ],
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    }
}